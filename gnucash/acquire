#!/usr/bin/env dl

gnucash-${UB_VERSION}.tar.bz2 gnucash-${UB_VERSION}/
	https://github.com/Gnucash/gnucash/releases/download/${UB_VERSION}/

${UB_VERSION}.tar.gz gnucash-docs-${UB_VERSION}/ :gnucash-docs
	https://github.com/Gnucash/gnucash-docs/archive/

gtest-1.10.0.tar.gz googletest-release-1.10.0/ :gtest
	https://github.com/google/googletest/archive/release-1.10.0.tar.gz
